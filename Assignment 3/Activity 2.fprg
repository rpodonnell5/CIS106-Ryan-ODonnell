    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="This program converts an inputted age into approximate total lived months, days, hours, minutes, and seconds"/>
            <declare name="age" type="Real" array="False" size=""/>
            <declare name="months" type="Real" array="False" size=""/>
            <declare name="days" type="Real" array="False" size=""/>
            <declare name="hours" type="Real" array="False" size=""/>
            <declare name="minutes" type="Real" array="False" size=""/>
            <declare name="seconds" type="Real" array="False" size=""/>
            <output expression="&quot;Please enter how old you are in years&quot;" newline="True"/>
            <input variable="age"/>
            <assign variable="months" expression="age*12"/>
            <assign variable="days" expression="age*365"/>
            <assign variable="hours" expression="age*365*24"/>
            <assign variable="minutes" expression="age*365*24*60"/>
            <assign variable="seconds" expression="age*365*24*60*60"/>
            <output expression="&quot;Your age is approximately equal to ...&quot;" newline="True"/>
            <output expression="&quot;Months = &quot; &amp; months &amp; &quot; months&quot;" newline="True"/>
            <output expression="&quot;Days = &quot; &amp; days &amp; &quot; days&quot;" newline="True"/>
            <output expression="&quot;Hours = &quot; &amp; hours &amp; &quot; hours&quot;" newline="True"/>
            <output expression="&quot;Minutes = &quot; &amp; minutes &amp; &quot; minutes&quot;" newline="True"/>
            <output expression="&quot;Seconds = &quot; &amp; seconds &amp; &quot; seconds&quot;" newline="True"/>
        </body>
    </function>
</flowgorithm>
